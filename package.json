  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "concurrently \"ng s --configuration=local\" \"npm run start:create-quotation\"  \"npm run start:auth\"  \"npm run start:hiring-proposal\"",
    "start:dev": "concurrently \"ng s --configuration=development\" \"npm run start:create-quotation --configuration=development\"  \"npm run start:auth --configuration=development\"  \"npm run start:hiring-proposal --configuration=development\"",
    "start:tst": "concurrently \"ng s --configuration=tst\" \"ng s create-quotation --configuration=tst\"  \"ng s auth --configuration=tst\"  \"ng s hiring-proposal --configuration=tst\"",
    "start:hml": "concurrently \"ng s --configuration=hml\" \"ng s create-quotation --configuration=hml\"  \"ng s auth --configuration=hml\"  \"ng s hiring-proposal --configuration=hml\"",
    "build": "ng build --configuration=development && ng build create-quotation --configuration=development && ng build hiring-proposal --configuration=development  && ng build auth --configuration=development",
    "build:tst": "ng build --configuration=tst && ng build create-quotation --configuration=tst && ng build hiring-proposal --configuration=tst && ng build auth --configuration=tst",
    "build:hml": "ng build --configuration=hml && ng build create-quotation --configuration=hml && ng build hiring-proposal --configuration=hml && ng build auth --configuration=hml",
    "build:prd": "ng build --configuration=production && ng build create-quotation --configuration=production && ng build hiring-proposal --configuration=production && ng build auth --configuration=production",
    "build:create-quotation": "ng build create-quotation --configuration=production",
    "build:hiring-proposal": "ng build hiring-proposal --configuration=production",
    "build:auth": "ng build auth --configuration=production",
    "start:create-quotation": "ng serve create-quotation --configuration=local",
    "start:hiring-proposal": "ng serve hiring-proposal --configuration=local",
    "start:auth": "ng serve auth --configuration=local",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "lint:all": "nx run-many --all --target=lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky install"
  },
